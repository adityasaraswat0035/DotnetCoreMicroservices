{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@5.0.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/"
    },
    {
      "library": "bootstrap-datetimepicker@4.17.47",
      "destination": "wwwroot/lib/bootstrap-datetimepicker/"
    },
    {
      "library": "moment.js@2.29.1",
      "destination": "wwwroot/lib/moment.js/"
    }
  ]
}